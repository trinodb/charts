{
  "categories": [
    {
      "title": "## Breaking Changes",
      "labels": [ "breaking-change" ]
    }
  ],
  "ignore_labels": [
    "release-note-none"
  ],
  "template": "## Changes\n\n${{UNCATEGORIZED}}\n\n${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}}. (#${{NUMBER}})",
  "base_branches": [
    "main"
  ],
  "tag_resolver": {
    "filter": {
      "pattern": "trino-(.+)"
    },
    "transformer": {
        "pattern": "trino-(.+)",
        "target": "$1"
    }
  }
}
